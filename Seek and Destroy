function destroyer(arr) {
 
  var initialArray=arr[0];
  var newArray=[];
  var numberArguments=arr.length-1; 
  var num;
  var argument;
  
  function checkEqual(num){
        return (num!==argument);
  }
  
  for (i=0;i<numberArguments;i++){
    argument=arr[i+1];
 newArray=initialArray.filter(function(num){
   return (num!==argument);
                              });
  
  }
return newArray;
}

destroyer([1, 2, 3, 1, 2, 3], 2, 3);
